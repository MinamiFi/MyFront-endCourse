<div class="product">
    <h2 class="product-title">精选课程</h2>
    <ul id="product-list" class="product-list">
        {{each productdata}}
        <li class="product-item">
            <a href="./details.html?id={{$value.id}}">
                <p class="product-pic">
                    <img src="{{$value.url}}" alt="" class="product-img">
                </p>
                <p class="product-name">{{$value.title}}</p>
            </a>
        </li>
        {{/each}}
    </ul>
</div>